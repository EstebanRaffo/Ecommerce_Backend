<h1>Listado de productos</h1>

{{#each products}}
    <div>Id: {{this._id}}, Producto: {{this.title}}, Precio: {{this.price}} <button onclick="addToCart(`{{this._id}}`)">Agregar al carrito</button></div>
{{/each}}
<a href="{{prevLink}}">Anterior</a>
<a href="{{nextLink}}">Siguiente</a>

<script src="/js/products.js"></script>